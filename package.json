{
	"name": "@keenmate/svelte-treeview",
	"version": "2.0.0",
	"repository": {
		"url": "https://github.com/keenmate/svelte-treeview"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test": "vitest",
		"test:loud": "vitest --silent=false",
		"coverage": "vitest run --coverage"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@keenmate/svelte-adminlte": "^1.1.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/marked": "^5.0.0",
		"@types/node": "^20.3.3",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"@vitest/coverage-v8": "^1.4.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"marked": "^5.1.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"publint": "^0.1.9",
		"rollup-plugin-string": "^3.0.0",
		"sass": "^1.49.0",
		"svelte": "^4.0.0",
		"svelte-check": "^3.4.3",
		"svelte-multiselect": "github:KeenMate/svelte-multiselect",
		"svelte-preprocess": "^5.0.4",
		"tslib": "^2.4.1",
		"typescript": "^5.4.4",
		"vite": "^5.0.0",
		"vitest": "^1.4.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@keenmate/js-common-helpers": "^1.2.0",
		"@types/lodash.unionby": "^4.8.9",
		"@types/lodash.uniq": "^4.5.9",
		"@types/lodash.uniqby": "^4.7.9",
		"lodash.unionby": "^4.8.0",
		"lodash.uniq": "^4.5.0",
		"lodash.uniqby": "^4.7.0",
		"svelte-highlight": "^7.6.1"
	},
	"overrides": {
		"summernote": "file:./EXPLICITLY_REMOVED"
	}
}
